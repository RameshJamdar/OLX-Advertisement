server:
  port: 9000

spring:
  datasource:
    driverClassName: com.mysql.jdbc.Driver
    url: jdbc:mysql://localhost:3306/olx_masterdata
    username: root
    password: root
  jpa:
    hibernate.ddl-auto: update
    generate-ddl: true
    show-sql: true
 
  config:
    import: "optional:configserver:"    
    

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
      
